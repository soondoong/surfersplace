<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="soo.member">
  	 	<insert id="memberInsert" parameterType="soo.member.model.MemberDTO">
  		insert into
  			g_member
  		values(g_member_idx.nextval,
  		#{name},
  		#{birth},
  		#{id},
  		#{pwd},
  		#{email},
  		#{addr},
  		#{joindate}
  		)
  	</insert>
  
  </mapper>